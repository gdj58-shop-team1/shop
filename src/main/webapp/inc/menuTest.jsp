					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">

						<a href="${pageContext.request.contextPath}/CartList" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-cart">
